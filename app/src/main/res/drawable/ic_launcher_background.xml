<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
  <group>
    <!-- Gradient background -->
    <path
        android:pathData="M0,0 L108,0 L108,108 L0,108 Z"
        android:fillColor="#4ADE80" />
    
    <!-- Overlay gradient for depth -->
    <path
        android:pathData="M0,0 L108,0 L108,108 L0,108 Z">
        <aapt:attr name="android:fillColor" xmlns:aapt="http://schemas.android.com/aapt">
            <gradient
                android:type="radial"
                android:gradientRadius="80"
                android:centerX="54"
                android:centerY="54"
                android:startColor="#06B6D4"
                android:centerColor="#4ADE80"
                android:endColor="#3B82F6" />
        </aapt:attr>
    </path>
    
    <!-- Subtle pattern overlay -->
    <path
        android:pathData="M0,0 L108,0 L108,108 L0,108 Z">
        <aapt:attr name="android:fillColor" xmlns:aapt="http://schemas.android.com/aapt">
            <gradient
                android:type="linear"
                android:angle="45"
                android:startColor="#20FFFFFF"
                android:centerColor="#10FFFFFF"
                android:endColor="#05FFFFFF" />
        </aapt:attr>
    </path>
  </group>
</vector>
